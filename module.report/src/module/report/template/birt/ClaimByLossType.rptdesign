            <xml-property name="queryText"><![CDATA[SELECT  company.id AS company_id, 
		company.code AS company_code, 
		UPPER(loss_type.name) as loss_type, 
		claim.id AS claim_id,
        claim.loss_date,
        'No of Cases' as periodRemark,
        case    when claim.status = 'CPAID' then 'paid'
                when CLAIM.STATUS IN ('OPN','PDOC','PADJ','PACC','POFR','PPYMT') then 'reserve'
                else 'not found'
        end as amt_source,
        case    when claim.status = 'CPAID' then approval_amt
                when CLAIM.STATUS IN ('OPN','PDOC','PADJ','PACC','POFR','PPYMT') then COALESCE(reserve_amount,0.00)
                else 0.00
        end as incurred_amnt
FROM claim
        INNER JOIN policy ON claim.policy_id = policy.id
        INNER JOIN insurance_class ON insurance_class.code = policy.insurance_class_code
        INNER JOIN insurance_class_category ON insurance_class_category.code = insurance_class.category
        INNER JOIN company ON company.id = policy.company_id
        INNER JOIN loss_type ON loss_type.code = claim.loss_type_code
extraCond
ORDER BY company.code, loss_type.code desc
]]></xml-property>
