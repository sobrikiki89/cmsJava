            <method name="beforeOpen"><![CDATA[importPackage( Packages.java.text );
var sdf = new SimpleDateFormat("dd-MM-yyyy HH:mm:ss", reportContext.getLocale());

var qrStr=" (claim.deleted is null or claim.deleted = false) ";

//User Company
var userCompany = reportContext.getParameterValue("userCompany");
if( (userCompany != null &amp;&amp; userCompany.length > 0) ){
	var userCompanyCodes = "";
	
	for(var i = 0; i < userCompany.length; i++) {
		userCompanyCodes += "'" + userCompany[i] + "',";
	}

	if(userCompanyCodes != "") {
		userCompanyCodes = userCompanyCodes.substring(0, userCompanyCodes.length - 1);
	}

	if(qrStr != "") {
		qrStr += " AND";
	}
	qrStr += " company.code IN (" + userCompanyCodes + ")";
}

// Companies
var companies = reportContext.getParameterValue("companies");
if( (companies != null &amp;&amp; companies.length > 0) ){
	var companyIds = "";
	
	for(var i = 0; i < companies.length; i++) {
		companyIds += companies[i] + ",";
	}

	if(companyIds != "") {
		companyIds = companyIds.substring(0, companyIds.length - 1);
	}
	
	if(qrStr != "") {
		qrStr += " AND";
	}
	
	qrStr += " company.id IN (" + companyIds + ")";
}

// From Loss Date, To Loss Date
var fromLossDate = reportContext.getParameterValue("fromLossDate");
var toLossDate = reportContext.getParameterValue("toLossDate");
if( (fromLossDate != null &amp;&amp; fromLossDate.value != "" &amp;&amp; toLossDate != null &amp;&amp; toLossDate.value != "") ){
	if(qrStr != "") {
		qrStr += " AND";
	}

	qrStr += " loss_date between to_timestamp('" + sdf.format( fromLossDate ) + "', 'DD-MM-YYYY HH24:MI:SS') and to_timestamp('" + sdf.format( toLossDate ) + "', 'DD-MM-YYYY HH24:MI:SS')";
}
else if( (fromLossDate != null &amp;&amp; fromLossDate.value != "" &amp;&amp; (toLossDate == null || toLossDate.value == "") ) ){
	if(qrStr != "") {
		qrStr += " AND";
	}

	qrStr += " loss_date >= to_timestamp('" + sdf.format( fromLossDate ) + "', 'DD-MM-YYYY HH24:MI:SS')";
}
else if( ( (fromLossDate == null || fromLossDate.value == "") &amp;&amp; toLossDate != null &amp;&amp; toLossDate.value != "" ) ){
	if(qrStr != "") {
		qrStr += " AND";
	}

	qrStr += " loss_date <= to_timestamp('" + sdf.format( toLossDate ) + "', 'DD-MM-YYYY HH24:MI:SS')";
}

// Group of Insurance
var groupOfInsurance = reportContext.getParameterValue("groupOfInsuranceCode");
if( (groupOfInsurance != null &amp;&amp; groupOfInsurance != "") ){
	qrStr += " AND insurance_class_category.code = '" + groupOfInsurance + "'";
}

// Class of Insurance
var insuranceClass = reportContext.getParameterValue("insuranceClass");
if( (insuranceClass != null &amp;&amp; insuranceClass.length > 0) ) {
	var insurerClassCodes = "";
	for(var i = 0; i < insuranceClass.length; i++) {
		insurerClassCodes += "'" + insuranceClass[i] + "',";
	}
	if(insurerClassCodes != "") {
	  insurerClassCodes = insurerClassCodes.substring(0, insurerClassCodes.length - 1);
	}
	qrStr += " AND insurance_class.code in (" + insurerClassCodes + ")";
}

// Loss Type
var lossType = reportContext.getParameterValue("lossTypeCode");
if( (lossType != null &amp;&amp; lossType.length > 0) ) {
	var lossTypeCodes = "";
	for(var i = 0; i < lossType.length; i++) {
		lossTypeCodes += "'" + lossType[i] + "',";
	}
	if(lossTypeCodes != "") {
	  lossTypeCodes = lossTypeCodes.substring(0, lossTypeCodes.length - 1);
	}
	qrStr += " AND loss_type.code in (" + lossTypeCodes + ")";
}

if (qrStr != "") {
	qrStr = " WHERE " + qrStr;
}

this.queryText = this.queryText.replace("extraCond", qrStr);
Packages.java.lang.System.out.println(queryText);
]]></method>
